{
  "name": "@allow2/allow2automate-ssh",
  "shortName": "ssh",
  "version": "0.0.2",
  "description": "Enable Allow2Automate the ability to use ssh to configure devices",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepublish": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Allow2/allow2automate-ssh"
  },
  "bugs": {
    "url": "https://github.com/Allow2/allow2automate-ssh/issues"
  },
  "homepage": "https://github.com/Allow2/allow2automate-ssh#readme",
  "allow2Token": "jdZL0mcSF8qq6kpr",
  "keywords": [
    "allow2automate",
    "allow2",
    "ssh",
    "plugin",
    "connectivity",
    "parental-controls",
    "network"
  ],
  "author": {
    "name": "Allow2",
    "url": "https://allow2.com"
  },
  "license": "MIT",
  "allow2automate": {
    "plugin": true,
    "pluginId": "allow2automate-ssh",
    "displayName": "SSH Remote Control",
    "category": "Connectivity",
    "permissions": [
      "network",
      "configuration"
    ],
    "minAppVersion": "2.0.0",
    "api": {
      "actions": [
        {
          "id": "connect",
          "name": "Connect SSH",
          "description": "Connect to remote device via SSH"
        },
        {
          "id": "executeRemote",
          "name": "Execute Remote Command",
          "description": "Execute command on remote device"
        },
        {
          "id": "enableAccess",
          "name": "Enable Access",
          "description": "Enable access on remote device"
        },
        {
          "id": "disableAccess",
          "name": "Disable Access",
          "description": "Disable access on remote device"
        }
      ],
      "triggers": [
        {
          "id": "connectionEstablished",
          "name": "Connection Established",
          "description": "Triggered when SSH connection is established"
        },
        {
          "id": "connectionFailed",
          "name": "Connection Failed",
          "description": "Triggered when SSH connection fails"
        },
        {
          "id": "commandExecuted",
          "name": "Command Executed",
          "description": "Triggered when remote command is executed"
        }
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "simple-ssh": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0",
    "@material-ui/core": "^4.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "rollup": "^2.40.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0"
  }
}
